{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/game-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/game-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/game-api-gateway",
  "projects": {
    "account": {
      "type": "application",
      "root": "apps/account",
      "entryFile": "main",
      "sourceRoot": "apps/account/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account/tsconfig.app.json"
      }
    },
    "game-api-gateway": {
      "type": "application",
      "root": "apps/game-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/game-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/game-api-gateway/tsconfig.app.json"
      }
    }
  }
}
